{
  "name": "getup",
  "version": "0.5.0",
  "description": "Frontend boilerplate",
  "author": "Genki Katsutani",
  "config": {
    "distDir": "dist"
  },
  "scripts": {
    "css-sass": "node-sass --output ${npm_package_config_distDir}/css --output-style expanded --include-path src/css --precision 6 'src/scss'",
    "css-postcss": "postcss --config postcss.config.js --replace ${npm_package_config_distDir}/css/**/*.css --verbose --no-map",
    "css": "npm-run-all css-sass css-postcss",
    "docs-styleguide": "postcss --config postcss.config.js --env guide --replace ${npm_package_config_distDir}/css/**/*.css --verbose",
    "docs": "npm-run-all --parallel docs-*",
    "move-js": "uglifyjs src/js/main.js -o ${npm_package_config_distDir}/js/main.js",
    "watch-bundle": "parcel watch src/js/main.js -o dist/js",
    "watch-scss": "onchange src/scss -- yarn run css",
    "watch-js": "onchange src/js -- yarn run move-js",
    "watch-html": "onchange src/templates src/_templates -- gulp nunjucks",
    "watch": "npm-run-all --parallel watch-*",
    "min-css": "postcss ${npm_package_config_distDir}/css/main.css --env minify -o ${npm_package_config_distDir}/css/main.css",
    "min": "npm-run-all --parallel min-*",
    "imagemin": "imagemin ${npm_package_config_distDir}/img/*.{jpg,png} --out-dir=${npm_package_config_distDir}/img",
    "copy-vendors-js": "node task/copy.js",
    "uglify-vendors-js": "uglifyjs src/js/vendors.js -o ${npm_package_config_distDir}/js/vendors.js",
    "copy": "npm-run-all --sequential copy-vendors-js uglify-vendors-js",
    "server": "node task/server.js",
    "start": "npm-run-all --parallel watch server"
  },
  "dependencies": {
    "animejs": "^3.0.0",
    "aos": "^2.3.4",
    "axios": "^0.18.0",
    "feature.js": "^1.0.0",
    "flexibility": "^2.0.1",
    "html5-boilerplate": "^7.0.1",
    "jquery": "^3.4.1",
    "lodash": "^4.17.11",
    "mobile-detect": "^1.4.3",
    "promise-polyfill": "^8.0.0",
    "riot": "^4.0.0-rc.7",
    "sanitize.css": "^8.0.0",
    "scroll-out": "^2.2.4",
    "slick-carousel": "^1.8.1",
    "smoothscroll-polyfill": "^0.4.4"
  },
  "directories": {
    "doc": "docs"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/nibushibu/Getup.git"
  },
  "bugs": {
    "url": "https://github.com/nibushibu/Getup/issues"
  },
  "homepage": "https://github.com/nibushibu/Getup",
  "license": "MIT",
  "devDependencies": {
    "@riotjs/cli": "^4.0.0",
    "@riotjs/compiler": "^4.0.0",
    "autoprefixer": "^9.5.1",
    "concat": "^1.0.3",
    "cpx": "^1.5.0",
    "css-mqpacker": "^7.0.0",
    "cssnano": "^4.1.5",
    "express": "^4.16.4",
    "gulp": "^4.0.0",
    "gulp-nunjucks-render": "^2.2.2",
    "node-sass": "^4.9.2",
    "npm-run-all": "^4.1.3",
    "onchange": "^5.0.2",
    "parcel-bundler": "^1.12.3",
    "parcel-plugin-riot": "^2.1.0",
    "perfectionist": "^2.4.0",
    "postcss": "^7.0.8",
    "postcss-cli": "^6.1.1",
    "postcss-devtools": "^1.1.1",
    "postcss-easings": "^2.0.0",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-focus": "^4.0.0",
    "postcss-import": "^12.0.0",
    "postcss-normalize-charset": "^4.0.0",
    "postcss-style-guide": "^0.14.0",
    "uglify-es": "^3.3.9"
  }
}
