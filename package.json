{
  "name": "getup",
  "version": "0.7.0",
  "description": "Frontend boilerplate",
  "author": "Genki Katsutani",
  "config": {
    "distDir": "dist"
  },
  "scripts": {
    "start": "parcel watch src/js/*.js --out-dir ${npm_package_config_distDir}/js --public-url ./ & yarn run css & yarn run nunjucks & yarn run watch & yarn run server",
    "css": "sass --style=expanded src/scss/main.scss ${npm_package_config_distDir}/css/main.css && postcss --config postcss.config.js --replace ${npm_package_config_distDir}/css/**/*.css --verbose --no-map",
    "watch": "onchange src/nunjucks -- yarn run nunjucks & onchange src/scss -- yarn run css",
    "nunjucks": "gulp nunjucks",
    "build": "parcel build src/js/main.js --out-dir ${npm_package_config_distDir}/js --no-source-maps && yarn run css && yarn run nunjucks",
    "server": "node node_scripts/serve.js"
  },
  "dependencies": {
    "@webcomponents/template": "^1.4.1",
    "animejs": "^3.1.0",
    "riot": "^4.9.0",
    "sanitize.css": "^11.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/nibushibu/Getup.git"
  },
  "bugs": {
    "url": "https://github.com/nibushibu/Getup/issues"
  },
  "homepage": "https://github.com/nibushibu/Getup",
  "license": "MIT",
  "devDependencies": {
    "@riotjs/compiler": "^4.6.0",
    "@riotjs/parcel-plugin-riot": "^4.0.1",
    "autoprefixer": "^9.7.4",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-runtime": "^6.23.0",
    "express": "^4.17.1",
    "gulp": "^4.0.2",
    "gulp-nunjucks-render": "^2.2.3",
    "mdcss": "^1.5.2",
    "onchange": "^6.1.0",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-riot": "^2.1.0",
    "postcss": "^7.0.26",
    "postcss-cli": "^7.1.0",
    "postcss-easings": "^2.0.0",
    "postcss-flexbugs-fixes": "^4.2.0",
    "postcss-focus": "^4.0.0",
    "postcss-import": "^12.0.1",
    "postcss-normalize-charset": "^4.0.1",
    "sass": "^1.25.0"
  }
}
