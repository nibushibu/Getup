{
  "name": "getup",
  "version": "1.5.1",
  "description": "Frontend boilerplate",
  "author": "Genki Katsutani",
  "scripts": {
    "start": "run-s riot start-watch",
    "start-watch": "run-p parcel-watch css watch:* server",
    "parcel-watch": "parcel watch 'src/js/main.js' --dist-dir 'dist/js' --public-url './' --no-hmr",
    "css": "postcss src/css/main.css --config postcss.config.js --output dist/css/main.css --map",
    "css-build": "postcss src/css/main.css --config postcss.config.js --output dist/css/main.css --no-map",
    "riot": "riot src/riot --config riot.config.js",
    "watch:css": "onchange src/css -- npm run css",
    "watch:riot": "riot src/riot --config riot.config.js --watch",
    "parcel-build": "parcel build 'src/js/main.js' --dist-dir 'dist/js' --no-source-maps",
    "build": "run-p parcel-build css-build",
    "server": "node node_scripts/serve.js"
  },
  "dependencies": {
    "animejs": "^3.2.1",
    "riot": "^5.0.0",
    "sanitize.css": "^12.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/nibushibu/Getup.git"
  },
  "bugs": {
    "url": "https://github.com/nibushibu/Getup/issues"
  },
  "homepage": "https://github.com/nibushibu/Getup",
  "license": "MIT",
  "devDependencies": {
    "@riotjs/cli": "^4.0.3",
    "@riotjs/compiler": "^5.0.0",
    "@riotjs/ssr": "^5.0.0",
    "@webcomponents/template": "^1.4.4",
    "cssnano": "^4.1.10",
    "express": "^4.17.1",
    "mkdirp": "^1.0.4",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "parcel": "^2.0.0-nightly.438",
    "postcss": "^8.1.4",
    "postcss-cli": "^8.2.0",
    "postcss-custom-properties": "^10.0.0",
    "postcss-flexbugs-fixes": "^5.0.0",
    "postcss-focus": "^5.0.1",
    "postcss-import": "^13.0.0",
    "postcss-normalize-charset": "^4.0.1",
    "postcss-preset-env": "^6.7.0"
  },
  "browserslist": [
    "last 2 version",
    "IE 11"
  ]
}
