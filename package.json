{
  "name": "getup",
  "version": "1.3.0",
  "description": "Frontend boilerplate",
  "author": "Genki Katsutani",
  "config": {
    "distDir": "dist"
  },
  "scripts": {
    "start": "parcel watch src/js/*.js --out-dir ${npm_package_config_distDir}/js --public-url ./ & npm run css & npm run nunjucks & npm run watch & npm run server",
    "css": "postcss src/css/main.css --config postcss.config.js --output ${npm_package_config_distDir}/css/main.css --map",
    "watch": "onchange src/nunjucks -- npm run nunjucks & onchange src/css -- npm run css",
    "nunjucks": "gulp nunjucks",
    "build": "parcel build src/js/main.js --out-dir ${npm_package_config_distDir}/js --no-source-maps && npm run css && npm run nunjucks",
    "server": "node node_scripts/serve.js"
  },
  "dependencies": {
    "animejs": "^3.2.0",
    "riot": "^4.12.4",
    "sanitize.css": "^11.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/nibushibu/Getup.git"
  },
  "bugs": {
    "url": "https://github.com/nibushibu/Getup/issues"
  },
  "homepage": "https://github.com/nibushibu/Getup",
  "license": "MIT",
  "devDependencies": {
    "@riotjs/compiler": "^4.8.5",
    "@riotjs/parcel-plugin-riot": "^4.0.1",
    "@webcomponents/template": "^1.4.2",
    "cssnano": "^4.1.10",
    "express": "^4.17.1",
    "gulp": "^4.0.2",
    "gulp-nunjucks-render": "^2.2.3",
    "mdcss": "^1.5.2",
    "onchange": "^7.0.2",
    "parcel-bundler": "^1.12.4",
    "postcss": "^7.0.31",
    "postcss-cli": "^7.1.1",
    "postcss-custom-properties": "^9.1.1",
    "postcss-easings": "^2.0.0",
    "postcss-flexbugs-fixes": "^4.2.1",
    "postcss-focus": "^4.0.0",
    "postcss-import": "^12.0.1",
    "postcss-normalize-charset": "^4.0.1",
    "postcss-preset-env": "^6.7.0"
  },
  "browserslist": [
    "last 2 version",
    "IE 11"
  ]
}
