{
  "name": "getup",
  "version": "0.5.0",
  "description": "Frontend boilerplate",
  "author": "Genki Katsutani",
  "config": {
    "distDir": "dist"
  },
  "scripts": {
    "css-sass": "node-sass --output ${npm_package_config_distDir}/css --output-style expanded --include-path src/css --precision 6 'src/scss'",
    "css-postcss": "postcss --config postcss.config.js --replace ${npm_package_config_distDir}/css/**/*.css --verbose --no-map",
    "css": "npm-run-all css-sass css-postcss",
    "docs-styleguide": "postcss --config postcss.config.js --env guide --replace ${npm_package_config_distDir}/css/**/*.css --verbose",
    "docs": "npm-run-all --parallel docs-*",
    "buble": "buble -i src/js -m -o ${npm_package_config_distDir}/js",
    "riot": "riot -w --config riot.config",
    "watch-riot": "yarn run riot",
    "watch-js": "onchange src/js -- yarn run buble",
    "watch-scss": "onchange src/scss -- yarn run css",
    "watch-html": "onchange src/ejs src/_ejs -- ejs-cli --base-dir src/ejs '**/*.ejs' --out ./dist",
    "watch": "npm-run-all --parallel watch-*",
    "min-css": "postcss ${npm_package_config_distDir}/css/main.css --env minify -o ${npm_package_config_distDir}/css/main.css",
    "min": "npm-run-all --parallel min-*",
    "imagemin": "imagemin ${npm_package_config_distDir}/img/*.{jpg,png} --out-dir=${npm_package_config_distDir}/img",
    "copy": "node task/copy.js"
  },
  "dependencies": {
    "@glidejs/glide": "^3.2.3",
    "animejs": "^2.2.0",
    "aos": "^2.3.4",
    "axios": "^0.18.0",
    "feature.js": "^1.0.0",
    "flexibility": "^2.0.1",
    "html5-boilerplate": "^6.1.0",
    "jquery": "^3.2.1",
    "lodash": "^4.17.11",
    "mobile-detect": "^1.4.3",
    "promise-polyfill": "^8.0.0",
    "riot": "^3.13.0",
    "riot-animore": "^1.0.0",
    "sanitize.css": "^8.0.0",
    "scroll-out": "^2.2.4",
    "slick-carousel": "^1.8.1"
  },
  "directories": {
    "doc": "docs"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/nibushibu/Getup.git"
  },
  "bugs": {
    "url": "https://github.com/nibushibu/Getup/issues"
  },
  "homepage": "https://github.com/nibushibu/Getup",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^9.1.3",
    "babel-core": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015-riot": "^1.1.0",
    "buble": "^0.19.4",
    "concat": "^1.0.3",
    "copy": "^0.3.2",
    "css-mqpacker": "^7.0.0",
    "cssnano": "^4.1.5",
    "ejs-cli": "^2.0.1",
    "node-sass": "^4.9.2",
    "npm-run-all": "^4.1.3",
    "onchange": "^5.0.2",
    "perfectionist": "^2.4.0",
    "postcss": "^7.0.5",
    "postcss-cli": "^6.0.1",
    "postcss-cssnext": "^3.0.2",
    "postcss-devtools": "^1.1.1",
    "postcss-easings": "^2.0.0",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-focus": "^4.0.0",
    "postcss-import": "^12.0.0",
    "postcss-normalize-charset": "^4.0.0",
    "postcss-style-guide": "^0.14.0"
  }
}
