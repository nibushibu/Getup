{
  "name": "getup",
  "version": "0.6.0",
  "description": "Frontend boilerplate",
  "author": "Genki Katsutani",
  "config": {
    "distDir": "dist"
  },
  "scripts": {
    "start": "parcel watch src/js/*.js --out-dir ${npm_package_config_distDir}/js & yarn run watch & node node_scripts/serve.js",
    "css": "node-sass --output ${npm_package_config_distDir}/css --output-style expanded --include-path src/css --precision 6 'src/scss' && postcss --config postcss.config.js --replace ${npm_package_config_distDir}/css/**/*.css --verbose --no-map",
    "watch": "onchange src/nunjucks -- yarn run nunjucks & onchange src/scss -- yarn run css",
    "nunjucks": "gulp nunjucks",
    "build": "parcel build src/js/main.js --out-dir ${npm_package_config_distDir}/js & yarn run css & yarn run nunjucks"
  },
  "dependencies": {
    "@riotjs/observable": "^4.0.1",
    "animejs": "^3.0.0",
    "aos": "^2.3.4",
    "axios": "^0.19.0",
    "express": "^4.17.1",
    "feature.js": "^1.0.0",
    "flexibility": "^2.0.1",
    "html5-boilerplate": "^7.0.1",
    "jquery": "^3.4.1",
    "lib": "^4.0.2",
    "mobile-detect": "^1.4.3",
    "parcel-plugin-riot": "^2.1.0",
    "promise-polyfill": "^8.0.0",
    "riot": "^4.3.5",
    "sanitize.css": "^11.0.0",
    "scroll-out": "^2.2.4",
    "slick-carousel": "^1.8.1",
    "smoothscroll-polyfill": "^0.4.4"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/nibushibu/Getup.git"
  },
  "bugs": {
    "url": "https://github.com/nibushibu/Getup/issues"
  },
  "homepage": "https://github.com/nibushibu/Getup",
  "license": "MIT",
  "devDependencies": {
    "@riotjs/compiler": "^4.3.5",
    "@riotjs/parcel-plugin-riot": "^4.0.1",
    "autoprefixer": "^9.6.0",
    "gulp": "^4.0.2",
    "gulp-nunjucks-render": "^2.2.3",
    "mdcss": "^1.5.2",
    "node-sass": "^4.9.2",
    "onchange": "^6.1.0",
    "parcel-bundler": "^1.12.3",
    "parcel-plugin-nunjucks": "^2.0.0",
    "postcss": "^7.0.8",
    "postcss-cli": "^6.1.3",
    "postcss-easings": "^2.0.0",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-focus": "^4.0.0",
    "postcss-import": "^12.0.0",
    "postcss-normalize-charset": "^4.0.0"
  }
}
