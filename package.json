{
  "name": "getup",
  "version": "0.7.0",
  "description": "Frontend boilerplate",
  "author": "Genki Katsutani",
  "config": {
    "distDir": "dist"
  },
  "scripts": {
    "start": "rollup -c -w 'src/**' & yarn run css & yarn run nunjucks & yarn run watch & yarn run server",
    "css": "node-sass --output ${npm_package_config_distDir}/css --output-style expanded --include-path src/css --precision 6 'src/scss' && postcss --config postcss.config.js --replace ${npm_package_config_distDir}/css/**/*.css --verbose --no-map",
    "watch": "onchange src/nunjucks -- yarn run nunjucks & onchange src/scss -- yarn run css",
    "nunjucks": "gulp nunjucks",
    "build": "rollup -c && yarn run css && yarn run nunjucks",
    "server": "browser-sync start --files 'dist/*, index.html' --server 'dist'"
  },
  "dependencies": {
    "animejs": "^3.0.0",
    "riot": "^4.5.1",
    "sanitize.css": "^11.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/nibushibu/Getup.git"
  },
  "bugs": {
    "url": "https://github.com/nibushibu/Getup/issues"
  },
  "homepage": "https://github.com/nibushibu/Getup",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "@riotjs/compiler": "^4.3.12",
    "@riotjs/parcel-plugin-riot": "^4.0.1",
    "autoprefixer": "^9.6.0",
    "browser-sync": "^2.26.7",
    "flexibility": "^2.0.1",
    "gulp": "^4.0.2",
    "gulp-nunjucks-render": "^2.2.3",
    "mdcss": "^1.5.2",
    "node-sass": "^4.9.2",
    "onchange": "^6.1.0",
    "postcss": "^7.0.8",
    "postcss-cli": "^6.1.3",
    "postcss-easings": "^2.0.0",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-focus": "^4.0.0",
    "postcss-import": "^12.0.0",
    "postcss-normalize-charset": "^4.0.0",
    "rollup": "^1.25.2",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-riot": "^4.0.0"
  }
}
